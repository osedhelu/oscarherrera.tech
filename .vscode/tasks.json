{
    "version": "0.0.1",
    "command": "bun",
    "isShellCommand": true,
    "args": [
        "--harmony"
    ],
    "tasks": [
        {
            "taskName": "runFile",
            "suppressTaskName": true,
            "showOutput": "always",
            "problemMatcher": "$jshint",
            "args": [
                "${file}"
            ],
            "label": "runFile"
        }
    ]
}